{
  "name": "@pvermeer/dexie-rxjs-addon",
  "version": "1.0.0-beta.13",
  "description": "Addon for Dexie.js database to add rxjs observables",
  "main": "dist/index.js",
  "module": "dist/esm-es5/index.js",
  "author": "PVermeer",
  "license": "MIT",
  "keywords": [
    "dexie",
    "dexie.js",
    "indexeddb",
    "database",
    "rxjs",
    "observable"
  ],
  "files": [
    "dist",
    "patches"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "init": "semantic-release-cli setup",
    "postinstall": "npm run patches",
    "build": "npm run clean && npm run build:bundles && npm run build:es6 && npm run build:esm-es5 && npm run build:declaration",
    "build:bundles": "webpack --config webpack.config.js",
    "build:esm-es5": "tsc -p tsconfig.esm-es5.json",
    "build:es6": "tsc -p tsconfig.es6.json",
    "build:declaration": "dts-bundle-generator --config dts-bundler.config.js",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run build && npm run lint && npm run test:jasmine && npm run test:karma",
    "test:karma": "karma start test/karma.conf.js",
    "test:karma:debug": "npm run build && karma start test/karma.debug.conf.js",
    "test:jasmine": "cd test && jasmine --require=ts-node/register --config=jasmine.conf.json --reporter=jasmine-console-reporter",
    "test:jasmine:debug": "cd test && nodemon --ext ts --exec jasmine --require=ts-node/register --config=jasmine.conf.json --reporter=jasmine-console-reporter",
    "test:coverage": "rimraf -rf reports/coverage/ && karma start test/karma.conf.js --browsers ChromeHeadless --reporters dots,coverage-istanbul",
    "clean": "rimraf -rf dist/ && rimraf -rf reports/",
    "semantic-release": "semantic-release",
    "patches": "path-exists node_modules && patch-package || cd ../../../ && patch-package --patch-dir node_modules/@pvermeer/dexie-rxjs-addon/patches"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "if git-branch-is -i -q -r \"master|next|next-major|beta|alpha|^v\\d+\\.\\d+(\\.\\d+)?(-\\S*)?$\"; then npm run test; fi"
    }
  },
  "dependencies": {
    "tslib": "^1.11.1",
    "dexie-observable": "^1.0.0-beta.6",
    "lodash": "^4.17.15",
    "patch-package": "^6.2.1",
    "path-exists-cli": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/travis-cli": "^8.3.5",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^8.0.0",
    "@types/faker": "^4.1.10",
    "@types/is-ci": "^2.0.0",
    "@types/jasmine": "^3.5.9",
    "@types/karma": "^3.0.8",
    "@types/lodash": "^4.14.149",
    "@types/lodash-es": "^4.17.3",
    "babel-loader": "^8.0.6",
    "dexie": "^3.0.0-rc.5",
    "dts-bundle-generator": "^4.0.0",
    "faker": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "git-branch-is": "^3.1.0",
    "husky": "^4.2.3",
    "is-ci": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.5.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-core": "^3.5.0",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-edgium-launcher": "github:PVermeer/karma-edgium-launcher#master",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "license-webpack-plugin": "^2.1.4",
    "lodash-es": "^4.17.15",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "semantic-release": "^17.0.4",
    "semantic-release-cli": "^5.2.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "dexie": "^3.0.0-rc.5",
    "rxjs": "^6.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PVermeer/dexie-rxjs-addon.git"
  },
  "bugs": {
    "url": "https://github.com/PVermeer/dexie-rxjs-addon/issues"
  },
  "homepage": "https://github.com/PVermeer/dexie-rxjs-addon#readme"
}
